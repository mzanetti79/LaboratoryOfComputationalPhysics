    "def f(l):\n",
    "    norm = []\n",
    "    sum=0\n",
    "    for x in l:\n",
    "            sum+=x\n",
    "    for i in range(len(l)):\n",
    "        l[i]=l[i]/sum\n",
    "        norm.append(l[i])\n",
    "    return norm\n",
    "\n",
    "l=[2,56,3,1]\n",
    "n=f(l)\n",
    "print(n)"
