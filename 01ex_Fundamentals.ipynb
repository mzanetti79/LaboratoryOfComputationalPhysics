    "n = int(input(\"Inserisci un numero di cui vuoi calcolare il fattoriale: \"))\n",
    "\n",
    "def fatt(n):\n",
    "    fatt = 1\n",
    "    if n >= 0:\n",
    "        while n>0:\n",
    "            fatt = fatt*n\n",
    "            n -= 1\n",
    "        return fatt\n",
    "\n",
    "def fatt_rec(n):\n",
    "    if n == 0 or n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*fatt_rec(n-1)\n",
    "\n",
    "print(fatt(n))\n",
    "print(fatt_rec(n))\n",
    "\n"
